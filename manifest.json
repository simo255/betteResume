{
    "manifest_version": 3,
    "name": "betteResume",
    "version": "1.0",
    "permissions": ["storage", "activeTab", "scripting"],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "host_permissions": [
        "*://*/*"
    ]
}
